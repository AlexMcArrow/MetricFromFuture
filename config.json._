{
    "rpc": {
        "front": {
            "active": true,
            "driver": "json",
            "dsn": {
                "type": "front",
                "host": "127.0.0.1",
                "port": 20744,
                "path": "/"
            }
        },
        "back": {
            "active": true,
            "driver": "json",
            "dsn": {
                "type": "back",
                "host": "127.0.0.1",
                "port": 20799,
                "path": "/"
            }
        }
    },
    "metric": {
        "test": {
            "active": true,
            "metric": {
                "interval": 1000,
                "driver": "dns",
                "path": {
                    "rec": "google.com",
                    "type": "A",
                    "dns": "8.8.8.8"
                }
            },
            "call": {
                "caller": "shell",
                "path": "echo $1 $2 $3 >> call.log"
            },
            "log": {
                "logger": "local"
            }
        }
    },
    "caller": {
        "shell": {
            "active": true,
            "driver": "shell",
            "path": "echo $1 $2 $3 >> $1.call.log"
        }
    },
    "logger": {
        "local": {
            "active": true,
            "driver": "file",
            "path": "./logs/$1.log"
        }
    }
}
